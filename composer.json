{
    "name": "aurawindsurfing/slashremovemiddleware",
    "description": "This package adds 301 redirect to all routes with trailing slashes.",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Lotocki",
            "email": "tom@gazeta.ie",
            "homepage": "https://github.com/aurawindsurfing"
        }
    ],
    "homepage": "https://github.com/aurawindsurfing/slashremovemiddleware",
    "keywords": ["Laravel", "SlashRemoveMiddleware"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Aurawindsurfing\\SlashRemoveMiddleware\\": "src/"
        }
